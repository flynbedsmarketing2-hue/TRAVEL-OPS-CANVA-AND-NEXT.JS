{
  "name": "travel-ops-oups",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "eslint . --max-warnings=0",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "npm run lint",
    "prisma:env-check": "node scripts/prisma-env-check.js",
    "prisma:generate": "npm run prisma:env-check && prisma generate",
    "db:push": "npm run prisma:env-check && prisma db push",
    "db:studio": "npm run prisma:env-check && prisma studio"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "html2pdf.js": "^0.12.1",
    "lucide-react": "^0.561.0",
    "next": "16.0.10",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "zod": "^3.22.2",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.19",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "eslint-config-prettier": "^9.1.0",
    "picocolors": "^1.1.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prisma": "^5.10.0",
    "tailwindcss": "^3.4.15",
    "typescript": "^5"
  }
}
